#include <iostream>
using namespace std;
                    
double Treug(int N,int K)
{
	double Sum = 1;
	for (int i = 1; i <= K; i++)
	Sum*=(N-i+1)/float(i);
	return Sum;
}
                  
int main()
{
    int n;
    cout<<"Enter N: ";
    cin>>n;
    cout<<" "<<endl;
 
for (int j = 0; j <= n; j++)
{
	for (int i=0; i <=j ; i++)
	cout<<Treug(j,i)<<" ";
	cout<<"\n";
}
    cin.get();
    cin.get();
 
    return 0;
}
