#include<iostream>
using namespace std;

long fuc(int a, int b)
{
    while (a && b)
        if (a >= b)
           a %= b;
        else
           b %= a;
    return a | b;
}
 
int main()
{
   int a;
   int b;
   cout<<"a=";
   cin>>a;
   cout<<"b=";
   cin>>b;
   cout<<"NOD ("<<a<<"; "<<b<<") = "<<fuc(a,b);
}
